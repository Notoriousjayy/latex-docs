@startuml
skinparam backgroundColor #FFFFFF
skinparam defaultFontName Arial
skinparam defaultFontSize 11
skinparam shadowing false

skinparam rectangle {
    RoundCorner 10
}

skinparam card {
    RoundCorner 8
}

title Software Element Types

rectangle "Software Elements" as main #F5F5F5 {
    
    together {
        card "Executables\n\nCompiled binaries\nInterpreted scripts\nBytecode files" as exec #E3F2FD
        card "Libraries\n\nShared (.so, .dll)\nStatic (.a, .lib)\nManaged assemblies" as libs #E3F2FD
    }
    
    together {
        card "Configuration\n\nXML, JSON, YAML\nINI, properties\nFeature flags" as config #FFF3E0
        card "Data Files\n\nLookup tables\nTemplates\nML models" as data #FFF3E0
    }
    
    together {
        card "Resources\n\nImages, icons\nFonts, stylesheets\nJS bundles" as res #E8F5E9
        card "Documentation\n\nUser manuals\nAPI docs\nLicense, README" as docs #E8F5E9
    }
    
    together {
        card "Runtime Generated\n\nLog files\nCache files\nTemporary data" as runtime #FCE4EC
        card "Plugins\n\nOptional extensions\nDynamically loaded\nFeature modules" as plugins #FCE4EC
    }
}

note right of exec
  Primary entry points
  for system functionality
end note

note right of config
  Controls behavior
  without code changes
end note

note right of runtime
  Created during
  system operation
end note

center footer Purpose: Classify artifacts by type to guide installation location decisions
@enduml
